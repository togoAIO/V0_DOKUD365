{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "api/index1.md"
        ]
      },
      {
        "files": [
          "ProcessDoku/**.md",
          "ProcessDoku/**/toc.yml",
          "ProcessDoku/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "CE/**.md",
          "CE/**/toc.yml",
          "CE/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "articles_de/**.md",
          "articles_de/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {

          "files": [
            "**/**/**.png",
            "**/**/**.jpg",
            "**/**/**.gif",
            "**/**.jpg",
            "**/**.jpeg",
            "**/**.gif",
            "**/**.svg"
          ]
        
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],

    "globalMetadata": {
      "_appTitle": "WIKA Documentation",
      "_enableSearch": "false",
      "_appFooter": "<span>WIKA Alexander Wiegand SE & Co. KG</span>"

      
    
    },
    
    "template":["default","templates/discordfx"],
  
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}